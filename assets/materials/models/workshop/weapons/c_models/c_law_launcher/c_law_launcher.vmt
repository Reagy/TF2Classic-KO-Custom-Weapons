"VertexLitGeneric"
{
	"$basetexture" "models/workshop/weapons/c_models/c_law_launcher/c_law_launcher"

	"$phong" "1"
	"$phongexponent" "15"
	"$phongboost" "3"	
	//"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
  //"$lightwarptexture" "models/weapons/c_items/c_ambassador_lightwarp"
  "$lightwarptexture" "models/lightwarps/softened_weapon_lightwarp"
	"$phongfresnelranges"	"[.5 .75 1]"
	"$basemapalphaphongmask" "1"

	"$rimlight" "1"
	"$rimlightexponent" "4"	
	"$rimlightboost" "1"

	"360?$color2" "[ 0.9 0.8 0.8 ]"


	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}