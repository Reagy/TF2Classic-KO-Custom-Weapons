"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\c_items\mgun.vtf"
	
	"$phong" "1"
	"$phongexponent" "15"
	"$phongboost" "0.75"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
//	"$lightwarptexture" "models\player\pyro\pyro_lightwarp"
	"$phongfresnelranges"	"[1.5 3 1]"
	"$basemapalphaphongmask" "1"
	
	"$rimlight" "1"
	"$rimlightexponent" "25"	
	"$rimlightboost" ".125"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}