"VertexLitGeneric"
{
	"$basetexture"		"models\weapons\c_items\c_whacktus/c_cactus_bat"
	"$bumpmap"		"models\weapons\c_items\c_whacktus/c_cactus_bat_normal"
	"$phong" "1"
	"$phongexponent" "40"
	"$phongboost" "1"	
	"$lightwarptexture" "models\lightwarps\softened_weapon_lightwarp"
	"$phongfresnelranges"		"[.25 .5 1]"
	"$basemapalphaphongmask" "0"

	"$rimlight" "1"
	"$rimlightexponent" "2"	
	"$rimlightboost" "4"


	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
