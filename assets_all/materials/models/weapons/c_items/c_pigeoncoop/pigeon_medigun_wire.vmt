"VertexLitGeneric"
{
	$baseTexture "models/weapons/c_items\c_pigeoncoop/pigeon_medigun_wire.vtf"
	"$bumpmap" "models\effects\flat_normal" 
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6
	"$yellow" "0"

	"$translucent" "1" 
//	"$alphatest" "1"
	"$nocull" "1"

 	"$blendtintbybasealpha" "0" 
 	"$blendtintcoloroverbase" "0"  

	"$colortint_base" "{40 36 24}"
	"$colortint_tmp" "[0 0 0]" 

	// Cloaking
	"$cloakPassEnabled" "1"

	"Proxies"
	{
		"weapon_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1"  "$yellow"
			"resultVar" "$color2"
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}

	}
}
