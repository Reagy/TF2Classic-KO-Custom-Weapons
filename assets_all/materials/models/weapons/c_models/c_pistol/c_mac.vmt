	"$tempvar" "0"
	
	"$basephongexponent" "15"
	"$basephongboost" "5"

	"$commadd_phongexponent" "40"
	"$commadd_phongboost" 	"22.5"
	"$commweapon"		"0"

	"$glowcolor" "1"
	
	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" ".125"			
	"$basemapalphaphongmask" "1"
	// Cloaking
	"$cloakPassEnabled" "1"
