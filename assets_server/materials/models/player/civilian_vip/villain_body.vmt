"VertexLitGeneric"
{
	"$basetexture" "models/player/villain/villain_body_both"
	"$bumpmap" "models/player/villain/villain_body_normal"
	"$frame" "0"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .0
	"$detailblendmode" 6
	"$bbqamount" "1"	
	"$two" "2.35"
	"$one" "1"
	"$absburnminusone" "1"
	"$burnminusone" "1"
	"$finalburnlevel" "1"
	"$burnlevel" "0"

	"$phong" "1"
//	"$phongexponenttexture" "models/player/villain/villain_body_expo"
	"$phongexponent" "15"
	"$phongboost" "1.5"	
	"$phongalbedotint" "1"	
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.3 1 8]"
	"$halflambert" "0"

	// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "4"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "2"										// Boost for ambient cube component of rim lighting

	"$cloakColorTint" "[1 0.5 0.4]"
	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"spy_invis"
		{
		}

		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}

		"BurnLevel"
		{
			"resultVar" "$burnlevel"
		}
		"Subtract"
		{
			"srcVar1" "$burnlevel"
			"srcVar2" "$one"
			"resultVar" "$burnminusone"
		}
		"Abs"
		{
			"srcVar1" "$burnminusone" 
			"resultVar" "$absburnminusone"
		}

		"Subtract"
		{
			"srcVar1" "$one"
			"srcVar2" "$absburnminusone"
			"resultVar" "$finalburnlevel"
		}


		"Equals"
		{
			"srcVar1" "$finalburnlevel"
			"resultVar" "$detailblendfactor"
		}
	

	}
}