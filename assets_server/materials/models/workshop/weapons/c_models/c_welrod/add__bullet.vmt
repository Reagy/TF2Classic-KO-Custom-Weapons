"VertexlitGeneric"
{
	//TexturePath
	"$baseTexture" "models\workshop\weapons\c_models\c_welrod\c_welrod"
	"$bumpmap" "models\workshop\weapons\c_models\c_welrod\c_welrod_normal"

	//Phong Parameters
	"$phong" "1"
	"$phongexponent" "20"
	"$phongboost" "2"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[1.5 .5 3]"
	"$halflambert" "1"
//	"$basemapalphaphongmask" "1"
	
	//this color is to distinguish different kinds of ammo, if that ends up a feature, comment it out completely otherwise
//	"$blendtintbybasealpha"		"1"
//	"$blendtintcoloroverbase"		"1"
//	"$color2"		"{ 33 33 44 }"
//	"$color2"		"{ 145 78 30 }"
//	"$color2"		"{ 64 84 40 }"
//	"$color2"		"{ 99 64 88 }"
//	"$color2"		"{ 219 198 127}"
//	"$color2"		"{ 222 222 222 }"

	//RimLighting Parameters
	"$rimlight" "1"
	"$rimlightexponent" "20"	
	"$rimlightboost" "1"		
			
	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	
	
		"WeaponSkin" {
		}
	}
}
