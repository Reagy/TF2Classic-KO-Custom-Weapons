
"VertexlitGeneric"
{
	"$basetexture" 	"models\workshop\weapons\c_models\c_quick_medigun/c_quick_medigun_backpack"

	"$envmap" "env_cubemap"
	"$envmaptint" "[.09 .09 .09]"
	$basemapalphaenvmapmask		"1"
$selfillum 1
$selfillummask "models\workshop\weapons\c_models\c_quick_medigun/c_quick_medigun_backpack_illum"



"$phong" "1"
	"$phongexponent" "13"
	"$phongboost" "3"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.25 2 1]"

	"$tempvar" "0"
	
	"$basephongexponent" "15"
	"$basephongboost" "5"

	"$commadd_phongexponent" "40"
	"$commadd_phongboost" 	"22.5"
	"$commweapon"		"0"

	"$glowcolor" "1"
	
	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" ".125"			
	"$basemapalphaphongmask" "1"



	"360?$color2" "[ 0.4 0.4 0.4 ]"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"Sine"
		{
			"resultVar" $selfillumtint
			"sinemin" .5
			"sinemax" 2.3
			"sineperiod" 2
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"


		}
	}
}