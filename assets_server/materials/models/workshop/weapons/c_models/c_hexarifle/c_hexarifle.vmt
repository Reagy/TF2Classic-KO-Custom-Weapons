"VertexlitGeneric"
{
	"$basetexture" "models/workshop/weapons/c_models/c_hexarifle/c_hexarifle"
	"$phongexponenttexture" "models/workshop/weapons/c_models/c_hexarifle/c_hexarifle_exp"
	"$selfillummask" "models/workshop/weapons/c_models/c_hexarifle/c_hexarifle_illum"
	"$basemapalphaphongmask" "1"
	"$phong" "1"
	"$phongexponent" "5"
	"$phongboost" "5"	
	"$phongexponentfactor"  "190"

	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
			
	"$phongfresnelranges"	"[.3 .5 3]"
	"$halflambert" "1"
	"$envmap" "env_cubemap"
	"$envmaptint" "[.05 .05 .05]"
	"$rimlight" "1"						
	"$rimlightexponent" "1"				
	"$rimlightboost" "2.25"
	"$rimmask" "1"
	"$tempvar" "0"
	"$selfillum" "1"
	"$selfillumtint" "[25 25 25]"

	"$glowcolor" "1"
	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	
	
		"WeaponSkin" {
		}
	}
}