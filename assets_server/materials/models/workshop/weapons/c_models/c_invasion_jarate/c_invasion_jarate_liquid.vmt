"VertexlitGeneric"
{
	"$basetexture" "models/workshop/weapons/c_models/c_invasion_jarate/c_invasion_jarate_liquid"
	//"$phongexponenttexture" "models/workshop/weapons/c_models/c_invasion_jarate/c_invasion_jarate_liquid_phongexponent"
	"$basemapalphaphongmask" "1"

	"$phong" "1"
	"$phongexponent" "5"
	"$phongboost" "1.5"	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"
	"$halflambert" "1"
	
	"$rimlight" "1"
	"$rimlightexponent" "4"	
	"$rimlightboost" "2"
	"$basephongexponent" "15"
	"$basephongboost" "2.4"
	"$phongalbedotint" "1"
	"$glowcolor" "1"
	"$selfillum" "1"
	"$selfillummaskscale" "[2 42]"
	"$detail"		"effects/tiledfire/fireLayeredSlowTiled512"
	"$detailscale"		"5"
	"$detailblendfactor"		"0"
	"$detailblendmode"		"6"
	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
		"TextureScroll"
		{
			"texturescrollvar"		"$basetexturetransform"
			"texturescrollrate"		"0.900000"
			"texturescrollangle"		"-90"
		}
		"Sine"
		{
			"sineperiod" "5"
			"sinemin" "0.2"
			"sinemax" "6"
			"resultVar" "$selfillumtint"
		}
	}
}



