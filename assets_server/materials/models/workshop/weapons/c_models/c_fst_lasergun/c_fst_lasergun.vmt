"VertexlitGeneric"
{
	"$baseTexture" "\models\workshop\weapons\c_models\c_fst_lasergun\c_fst_lasergun.vtf"
	"$bumpmap"		"\models\workshop\weapons\c_models\c_fst_lasergun\c_fst_lasergun_spec.vtf"	
	
	"$selfillum" "1"

	"$phong" "1"
	"$phongexponent" "15"
	"$phongboost" "5"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"

	"$rimlight" "1"
	"$rimlightexponent" "15"	
	"$rimlightboost" ".5"	
	
	"360?$color2" "[ 0.9 0.8 0.8 ]"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	
	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}