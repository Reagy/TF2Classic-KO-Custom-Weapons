"VertexlitGeneric"
{
	"$baseTexture" "models/workshop/weapons/c_models/c_moonbeam/c_moonbeam_lens"

	"$envmap" "env_cubemap"
	"$envmaptint" "[.7 .6 .6]"

	"$phong" "1"
	"$phongexponent" "35"
	"$phongboost" "6"	
	"$lightwarptexture" "models/lightwarps/jarate_lightwarp"

	"$phongfresnelranges"	"[.25 .5 1]"

	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" "2"

	"360?$color2" "[ 0.5 0.5 0.5 ]"
	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}

