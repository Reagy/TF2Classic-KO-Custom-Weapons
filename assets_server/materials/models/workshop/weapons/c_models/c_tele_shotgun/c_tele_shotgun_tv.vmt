"UnlitTwoTexture"
{
	"$baseTexture" "models/workshop/weapons/c_models/c_tele_shotgun/c_tele_shotgun_tv"
	"$texture2"  "models/workshop/weapons/c_models/c_tele_shotgun/c_tele_shotgun_gradient"
	
	"$glowcolor" "1"
	"$cloakPassEnabled" "1"
	"$txScale" "1"

	"$yellow" "0"

	"Proxies"
	{	
		"TextureScroll"
		{
		"texturescrollvar" "$texture2transform"
		"texturescrollrate" -.6
		"texturescrollangle" 0
		}
		"TextureScroll"
		{
		"texturescrollvar" "$basetexturetransform"
		"texturescrollrate" -.1
		"texturescrollangle" 0
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
		"BuildingRescueLevel"
		{
			"resultVar" "$basetexturetransform"
		}
	}
}
