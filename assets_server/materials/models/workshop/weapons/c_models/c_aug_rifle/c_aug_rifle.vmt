{
	"$basetexture" "models/workshop/weapons/c_models/c_aug_rifle/c_aug_rifle"
	"$phongexponenttexture" "models/workshop/weapons/c_models/c_aug_rifle/c_aug_rifle_expmap"
	"$selfillummask"  "models/workshop/weapons/c_models/c_aug_rifle/c_aug_rifle_illum"
	
	"$selfillum" "1"
	"$phongalbedotint" "1"
	"$basemapalphaphongmask" "1"

	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "105"	
	"$lightwarptexture" "models/weapons/c_items/c_ambassador_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"
	"$halflambert" "1"
	"$rimlight" "1"
	"$rimlightexponent" "7"	
	"$rimlightboost" "25.25"


	"$glowcolor" "1"
	"$selfillumtint" "[6 6 6]"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
