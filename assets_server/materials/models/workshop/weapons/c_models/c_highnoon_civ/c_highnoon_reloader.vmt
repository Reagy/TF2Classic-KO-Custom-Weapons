"VertexlitGeneric"
{
	"$baseTexture" "models/workshop/weapons/c_models/c_highnoon_civ/c_highnoon_reloader"

	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"

	//"$envmap" "env_cubemap"
	//"$envmaptint" "[.2 .16 .2]"
	//"$basemapalphaenvmapmask" "1"

	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "5"
	"$basemapalphaphongmask" "1"	

	"$phongfresnelranges"	"[.25 .5 1]"
	"$halflambert" "1"
	"$glowcolor" "1"

	"$tempvar" "0"

	"$commadd_phongexponent" "40"
	"$commadd_phongboost" 	"22.5"
	"$commweapon"		"0"
	
	"$rimlight" "1"						
	"$rimlightexponent" "50"				
	"$rimlightboost" ".1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"


	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"CommunityWeapon"
		{
			"resultVar" "$commweapon"
		}
		"Multiply"
		{
			"srcVar1"  "$commweapon"
			"srcVar2"  "$commadd_phongexponent" 
			"resultVar" "$tempvar" 
		}
		"Add"
		{
			"srcVar1"  "$tempvar" 
			"srcVar2"  "$basephongexponent" 
			"resultVar" "$phongexponent" 
		}
		"Multiply"
		{
			"srcVar1"  "$commweapon"
			"srcVar2"  "$commadd_phongboost" 
			"resultVar" "$tempvar" 
		}
		"Add"
		{
			"srcVar1"  "$tempvar" 
			"srcVar2"  "$basephongboost"
			"resultVar" "$phongboost" 
		}
	}
}