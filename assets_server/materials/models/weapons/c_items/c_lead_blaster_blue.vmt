"VertexlitGeneric"
{
	"$basetexture" "models/weapons/c_items/c_lead_blaster_blue"
  //"$bumpmap" "models/weapons/c_items/c_lead_blaster_normal"

	"$basemapalphaphongmask" "1"
	"$phong" "1"
	"$phongexponent" "15"
	"$phongboost" "8"	
	//"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
  "$lightwarptexture" "models/weapons/c_items/c_ambassador_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"
  
  //"$envmap"			"env_cubemap"
	//"$envmaptint"			"[.3 .3 .3]"
  //"$envmapmask" "models/weapons/c_items/c_lead_blaster_envmask"
  //"$normalmapalphaenvmapmask" "1" // This doesn't seem to work with Phong enabled in the alpha channel!
  //"$basealphaenvmapmask" "0"

	"$rimlight" "1"
	"$rimlightexponent" "10"	
	"$rimlightboost" "1.5"

	"$ambientocclusion" "1"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
	}
}

