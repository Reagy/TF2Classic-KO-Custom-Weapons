"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\c_items/c_sharkjaw"
	"$bumpmap" "models\weapons\c_items/c_sharkjaw_normal"

	"$envmap" "env_cubemap"
	"$envmaptint" "[ 0.1 0.1 0.1]"

	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "5"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"

	"$basealphaenvmapmask" 1
	"$normalmapalphaphongmask" 1

	"$basephongexponent" "15"
	"$basephongboost" "5"

	"$rimlight" "1"
	"$rimlightexponent" "4"	
	"$rimlightboost" "2"

	"$glowcolor" "1"

	"$cloakPassEnabled" "1"

	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}