		"$envmap" "env_cubemap"
	"$envmaptint" "[.085 .085 .1]"
	"$basemapalphaenvmapmask" "1"
                "$basemapalphaphongmask" "1"
	"$phong" "1"
	"$phongexponent" "30"
	"$phongboost" "6"	

	"$phongfresnelranges"	"[.25 .5 1]"
	"$halflambert" "1"
	"$glowcolor" "1"

	"$tempvar" "0"

	"$commadd_phongexponent" "30"
	"$commadd_phongboost" 	"22.5"
	"$commweapon"		"0"
	
	"$rimlight" "1"						
	"$rimlightexponent" "4"				
	"$rimlightboost" ".5"

	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"CommunityWeapon"
		{
			"resultVar" "$commweapon"
		}
		"Multiply"
		{
			"srcVar1"  "$commweapon"
			"srcVar2"  "$commadd_phongexponent" 
			"resultVar" "$tempvar" 
		}
		"Add"
		{
			"srcVar1"  "$tempvar" 
			"srcVar2"  "$basephongexponent" 
			"resultVar" "$phongexponent" 
		}
		"Multiply"
		{
			"srcVar1"  "$commweapon"
			"srcVar2"  "$commadd_phongboost" 
			"resultVar" "$tempvar" 
		}
		"Add"
		{
			"srcVar1"  "$tempvar" 
			"srcVar2"  "$basephongboost"
			"resultVar" "$phongboost" 
		}
	}
