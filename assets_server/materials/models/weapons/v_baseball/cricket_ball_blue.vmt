"VertexLitGeneric"
{
 	"$basetexture" "models/weapons/v_baseball/cricket_ball_blue"
	"$bumpmap" "models/weapons/v_baseball/cricket_ball_normal"
	"$normalmapalphaenvmapmask" 1

	"$phong" "1"
	"$phongexponent" "20"
	"$phongboost" "1.3"	
	"$lightwarptexture" "models\player\pyro\pyro_lightwarp"
	"$phongfresnelranges"	"[.05 .15 .25]"

	"$rimlight" "1"
	"$rimlightexponent" "5"	
	"$rimlightboost" "1"


	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}

}
