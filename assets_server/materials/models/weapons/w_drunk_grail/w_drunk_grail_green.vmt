"VertexLitGeneric"
{
	"$baseTexture"	"models/weapons/w_drunk_grail/w_drunk_grail_green"
	"$bumpmap"		"models/weapons/w_drunk_grail/w_drunk_grail_normal"
	"$surfaceprop"	"metal"
	
	"$phong"				"1"
	"$phongexponent"		"20"
	"$phongboost"			"4"	
	"$phongfresnelranges"	"[.25 .5 1]"
	"$lightwarptexture"		"models/player/pyro/pyro_lightwarp"
	
	"$rimlight" 			"1"												
	"$rimlightexponent"		"4"					
	"$rimlightboost"		"2"	
	
	"$envmap"		"env_cubemap"
	"$envmaptint"	"[.5 0 0]"
	
	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 			"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 			"0"

	"$glowcolor"	"1"
	"$yellow"		"0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}