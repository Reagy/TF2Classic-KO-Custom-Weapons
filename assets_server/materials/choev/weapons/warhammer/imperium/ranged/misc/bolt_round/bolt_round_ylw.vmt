"VertexLitGeneric"
{
//Main
	$basetexture					"ChoEv/Weapons/Warhammer/Imperium/Ranged/Misc/Bolt_Round/Bolt_Round_YLW_D"
	$bumpmap						"ChoEv/Weapons/Warhammer/Imperium/Ranged/Misc/Bolt_Round/Bolt_Round_N"

//Other
	$lightwarptexture				"models/lightwarps/weapon_lightwarp"
	$halflambert					"1"
	$normalmapalphaphongmask		"1"
	$glowcolor						"1"

//Phong
	$phong							"1"
	$phongexponent					"15"
	$phongboost						"1"
	$phongfresnelranges				"[.25 .5 1]"

//Rimlight
	$rimlight						"1"
	$rimlightexponent				"4"
	$rimlightboost					"2"

//Cloaking
	$cloakPassEnabled				"1"
	$sheenPassEnabled				"1"

//KS Sheen
	$sheenmap						"cubemaps/cubemap_sheen001"
	$sheenmapmask					"Effects/AnimatedSheen/animatedsheen0"
	$sheenmaptint					"[ 1 1 1 ]"
	$sheenmapmaskframe				"0"
	$sheenindex						"0"

	$yellow							"0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar"			"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
		"WeaponSkin" {
		}
	}
