"VertexLitGeneric"
{
	"$baseTexture" "sharp_move/c_sharp_move_tc_yellow"
	"$bumpmap" "sharp_move/c_sharp_move_tc_normal"
	"$bumpmapalphaphongmask" 1

	"$phong" "1"
